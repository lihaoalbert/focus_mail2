<!--发送活动外包装 -->
<script src="/assets/application.js" type="text/javascript"></script>
<div class="stream-manager js-stream-manager-container">
	<div class="js-stream-manager" id="profile-stream-manager">
		<div class="stream-title">
			
		</div>
		<div class="stream-container">
			<div class="stream ">
				<div class="js-stream-items stream-items" id="stream-items-id">
				<% if @campaigns != nil && @campaigns != "" %>
		  			<% @campaigns.each do |c| %>
		  			<!--单条发送活动 -->
		    		<div class="js-stream-item stream-item" data-item-type="tweet" media="true"> 
		    			<div class="stream-item-content tweet js-actionable-tweet js-stream-tweet stream-tweet favorited  " data-is-reply-to="false" >
		      				<div class="tweet-image">
		      					<%= high_chart("chart" + c[0].to_s , c[1]) %>
				      		</div> 
				 			<div class="tweet-content">
    							<div class="tweet-row">
								  <span class="campaign-name">
								    <a href="#">发信活动<%=h c[0] %></a>
								  </span>
				   				</div>
				   				<div class="tweet-row">
				      			  <div class="tweet-text js-tweet-text">
				      			  	总发送量：<%=h c[2] %>
				      			  </div>
				   				</div>
				   				<div class="tweet-row">
				      			  <div class="tweet-text js-tweet-text">
				      			  	到达量：<%=h c[3] %>
				      			  </div>
				   				</div>
				   				<div class="tweet-row">
				      			  <div class="tweet-text js-tweet-text">
				      			  	硬弹回量：<%=h c[4] %>
				      			  </div>
				   				</div>
				   				<div class="tweet-row">
				      			  <div class="tweet-text js-tweet-text">
				      			  	软弹回量：<%=h c[5] %>
				      			  </div>
				   				</div>
					        </div>
						</div>
					</div>
					<!--单条发送活动 结束-->       
		  			<% end %>
		  		<% end %>
		  		</div>
			</div>
		</div>
	</div>
</div>


<script type="text/javascript">
	var strhost1 = window.location.host;
	var strprotocol1 = window.location.protocol;
	
</script>
