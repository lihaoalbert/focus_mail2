<div id="report_title" style="margin-bottom: 20px">
		<h2 style="display:inline"> <%= @tracks[0].CampaignName %>打开邮件用户名单： </h2>
		<div style="float:right">
				  <a onclick="$('#track_form').fadeOut().html('');" class='btn btn-primary'>导出</a>
				</div>
</div>
<table class="table table-striped" id="campaigns">
  <thead>
    <tr>
      <th>MemName</th>
      <th>MemEmail</th>
      <th>CampaignName</th>
    </tr>
  </thead>
  <tbody>
    <% @tracks.each do |track| %>
      <tr>
        <td><%= track.MemName %></td>
        <td><%= track.MemEmail %></td>
        <td><%= track.CampaignName %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= will_paginate @tracks, :render => 'WillPaginate::LinkRender' %>

<script>
$(function() {
  $(".pagination a").live("click", function() {
    $(".pagination").html("data is loading...");
    $.get(this.href, null, null, "script");
    return false;
  });
});
</script>
